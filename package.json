{
  "name": "story-clash-codex",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": ">=20 <21"
  },
  "scripts": {
    "dev": "node --import tsx server/index.ts",
    "build": "next build",
    "start": "NODE_ENV=production node --import tsx server/index.ts",
    "lint": "eslint src server tests --ext .ts,.tsx --max-warnings=0",
    "typecheck": "tsc --noEmit",
    "typecheck:worker": "tsc -p cloudflare/tsconfig.json --noEmit",
    "test": "vitest run",
    "test:e2e": "playwright test",
    "dev:worker": "wrangler dev --config cloudflare/wrangler.toml",
    "deploy:worker": "wrangler deploy --config cloudflare/wrangler.toml"
  },
  "dependencies": {
    "@sentry/nextjs": "^10.38.0",
    "@swc/helpers": "0.5.18",
    "clsx": "^2.1.1",
    "express": "^5.1.0",
    "framer-motion": "^12.23.24",
    "nanoid": "^5.1.6",
    "next": "14.2.35",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "zod": "^4.3.6",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20260207.0",
    "@playwright/test": "^1.58.2",
    "@types/express": "^5.0.5",
    "@types/node": "^20.19.24",
    "@types/react": "^18.3.24",
    "@types/react-dom": "^18.3.7",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.1",
    "eslint-config-next": "14.2.35",
    "playwright": "^1.58.2",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18",
    "wrangler": "^4.63.0"
  }
}
